pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
popped=nil

stack={
  "1","2","3","4"
}

function push(_stack,_arg)
  _stack[#_stack+1]=_arg
end

function pop(_stack)
  local _pop=_stack[#_stack]
  _stack[#_stack]=nil
  return _pop
end

function multipop(_stack,_n)
  local _pops={}
  local _pop=nil
  for i=1,_n do
    _pop=pop(_stack)
    push(_pops,_pop)
  end
  return _pops
end

function _init()
  push(stack,5.1234)
  push(stack,"game state")
  popped=multipop(stack,2)
end

function _draw()
  cls()
  printtable(stack,10,10)
  printtable(popped,10,20)
end

function printtable(table,x,y)
  local str=""
  if type(table)=="table" then
	  for n in all(table) do
	    str=str..n..", "
	  end
	 elseif type(table)=="number" then
	   str=table
	 end
  print(str,x,y)
end
